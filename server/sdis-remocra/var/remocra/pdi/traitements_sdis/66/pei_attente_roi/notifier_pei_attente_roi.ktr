    <connection>Remocra (Postgis)</connection>
    <sql>SELECT DISTINCT zcc.commune_id AS id_commune, o.email_contact, o.nom AS nom_destinataire
FROM remocra.organisme o
	INNER JOIN remocra.zone_competence_commune zcc ON zcc.zone_competence_id = o.zone_competence
	JOIN remocra.type_organisme tor ON tor.id = o.type_organisme
		AND tor.code = 'CIS'
WHERE o.email_contact &lt;&gt; '' 
	AND o.email_contact IS NOT NULL
ORDER BY id_commune</sql>
