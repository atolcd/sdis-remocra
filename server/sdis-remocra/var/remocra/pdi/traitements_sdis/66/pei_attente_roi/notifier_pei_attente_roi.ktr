    </partitioning>
    <connection>Remocra (Postgis)</connection>
    <sql>SELECT h.id AS id_hydrant,
    h.commune AS commune_id,
    c.nom AS commune,
    h.numero,
    h.voie
FROM remocra.hydrant h
	JOIN remocra.commune c ON c.id = h.commune
	JOIN remocra.organisme o ON o.code = c.insee 
	LEFT JOIN (SELECT hv.date, hv.hydrant
		FROM remocra.hydrant_visite hv
			JOIN remocra.type_hydrant_saisie ths ON ths.id = hv.type
				AND ths.code = 'CREA') AS recep ON recep.hydrant = h.id
	LEFT JOIN (SELECT hv.date, hv.hydrant
		FROM remocra.hydrant_visite hv
			JOIN remocra.type_hydrant_saisie ths ON ths.id = hv.type
				AND ths.code = 'RECEP') AS roi ON roi.hydrant = h.id
WHERE recep.date IS NOT NULL
