  <step>
    <name>pei_indispo</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Remocra (Postgis)</connection>
    <sql>WITH territoire AS (
	SELECT o.id, zc.geometrie
	FROM remocra.organisme o
		JOIN remocra.zone_competence zc ON zc.id = o.zone_competence
	WHERE o.id = ${ORGANISME_ID}
)
SELECT
	xmlelement(name "alerte",
		indispo.pei,
		nonconforme.pei,
		dispo.pei
	) AS xml
FROM
-- Hydrants en état d'indisponibilité
(SELECT xmlelement(name "indispo",
			xmlagg( xmlelement(name "pei",
				xmlelement(name "numero", h_indispo.numero),
				xmlelement(name "voie", h_indispo.voie),
				xmlelement(name "type", h_indispo.type),
				xmlelement(name "anomalies", h_indispo.anomalies),
				xmlelement(name "commune", h_indispo.commune)
			))
		) AS pei
FROM
(SELECT
	h.id AS id_hydrant,
	h.numero,
	h.voie,
	thn.nom AS type,
	anomalie.anomalies,
	c.nom AS commune
FROM
	remocra.hydrant h
	INNER JOIN territoire t ON ST_Contains(t.geometrie, h.geometrie)
	INNER JOIN remocra.commune c ON c.id = h.commune
	INNER JOIN remocra.type_hydrant_nature thn ON thn.id = h.nature
	LEFT JOIN (
		SELECT
			a.hydrant,
			array_to_string(array_agg(ta.nom),', ') AS anomalies
		FROM
			remocra.hydrant_anomalies a
			JOIN remocra.type_hydrant_anomalie ta ON (ta.id = a.anomalies)
		GROUP BY
			a.hydrant) AS anomalie ON(anomalie.hydrant = h.id)
WHERE
	dispo_terrestre = 'INDISPO'
) AS h_indispo
JOIN (
-- Première date de bascule en indisponibilité après la
-- dernière date de disponibilité totale ou partielle connue
SELECT
	h.id_hydrant,
	MIN(h.date_operation) AS date_operation
FROM
	tracabilite.hydrant h
	JOIN (--Dernière date de disponibilite totale ou partielle connue 
		SELECT
			h.id_hydrant,
			max(h.date_operation) AS date_operation
		FROM
			tracabilite.hydrant h
		WHERE
			dispo_terrestre &lt;&gt; 'INDISPO' AND dispo_terrestre IS NOT NULL
		GROUP BY
			h.id_hydrant) dispo ON(dispo.id_hydrant = h.id_hydrant AND dispo.date_operation &lt; h.date_operation)
WHERE
	dispo_terrestre = 'INDISPO'
GROUP BY
	h.id_hydrant) AS der_etat_dispo ON(der_etat_dispo.id_hydrant = h_indispo.id_hydrant)
WHERE der_etat_dispo.date_operation >= now() - interval '${PDI_BASCULE_PERIODE} hour') AS indispo,
(SELECT xmlelement(name "non_conforme",
			xmlagg( xmlelement(name "pei",
				xmlelement(name "numero", h_nc.numero),
				xmlelement(name "voie", h_nc.voie),
				xmlelement(name "type", h_nc.type),
				xmlelement(name "commune", h_nc.commune)
			))
		) AS pei
FROM (SELECT
	h.id AS id_hydrant,
	h.numero,
	h.voie,
	thn.nom AS type,
	c.nom AS commune
FROM
	remocra.hydrant h
	INNER JOIN territoire t ON ST_Contains(t.geometrie, h.geometrie)
	INNER JOIN remocra.commune c ON c.id = h.commune
	INNER JOIN remocra.type_hydrant_nature thn ON thn.id = h.nature
WHERE
	dispo_terrestre = 'NON_CONFORME'
) AS h_nc
JOIN (
-- Première date de bascule en indisponibilité après la
-- dernière date de disponibilité totale ou partielle connue
SELECT
	h.id_hydrant,
	MIN(h.date_operation) AS date_operation
FROM
	tracabilite.hydrant h
	JOIN (--Dernière date de disponibilite totale ou partielle connue 
		SELECT
			h.id_hydrant,
			max(h.date_operation) AS date_operation
		FROM
			tracabilite.hydrant h
		WHERE
			dispo_terrestre &lt;&gt; 'NON_CONFORME' AND dispo_terrestre IS NOT NULL
		GROUP BY
			h.id_hydrant) dispo ON(dispo.id_hydrant = h.id_hydrant AND dispo.date_operation &lt; h.date_operation)
WHERE
	dispo_terrestre = 'NON_CONFORME'
GROUP BY
	h.id_hydrant) AS der_etat_dispo ON(der_etat_dispo.id_hydrant = h_nc.id_hydrant)
WHERE der_etat_dispo.date_operation >= now() - interval '${PDI_BASCULE_PERIODE} hour'
) AS nonconforme,
(SELECT xmlelement(name "dispo",
			xmlagg( xmlelement(name "pei",
				xmlelement(name "numero", h_dispo.numero),
				xmlelement(name "voie", h_dispo.voie),
				xmlelement(name "type", h_dispo.type),
				xmlelement(name "commune", h_dispo.commune)
			))
		) AS pei
FROM (SELECT
	h.id AS id_hydrant,
	h.numero,
	h.voie,
	thn.nom AS type,
	c.nom AS commune
FROM
	remocra.hydrant h
	INNER JOIN territoire t ON ST_Contains(t.geometrie, h.geometrie)
	INNER JOIN remocra.commune c ON c.id = h.commune
	INNER JOIN remocra.type_hydrant_nature thn ON thn.id = h.nature
WHERE
	dispo_terrestre = 'DISPO'
) AS h_dispo
JOIN (
-- Première date de bascule en indisponibilité après la
-- dernière date de disponibilité totale ou partielle connue
SELECT
	h.id_hydrant,
	MIN(h.date_operation) AS date_operation
FROM
	tracabilite.hydrant h
	JOIN (--Dernière date de disponibilite totale ou partielle connue 
		SELECT
			h.id_hydrant,
			max(h.date_operation) AS date_operation
		FROM
			tracabilite.hydrant h
		WHERE
			dispo_terrestre &lt;&gt; 'DISPO' AND dispo_terrestre IS NOT NULL
		GROUP BY
			h.id_hydrant) dispo ON(dispo.id_hydrant = h.id_hydrant AND dispo.date_operation &lt; h.date_operation)
WHERE
	dispo_terrestre = 'DISPO'
GROUP BY
	h.id_hydrant) AS der_etat_dispo ON(der_etat_dispo.id_hydrant = h_dispo.id_hydrant)
WHERE der_etat_dispo.date_operation >= now() - interval '${PDI_BASCULE_PERIODE} hour'
) AS dispo</sql>
