    <connection>Remocra (Postgis)</connection>
    <sql>WITH cis AS (
	SELECT o.id, zc.geometrie
	FROM remocra.organisme o
		JOIN remocra.zone_competence zc ON zc.id = o.zone_competence
	WHERE o.id = ${ORGANISME_ID}
),
profil_utilisateur AS (
	SELECT *
	FROM remocra.profil_utilisateur
	WHERE code IN (${PROFILS})
)
SELECT u.prenom || ' ' || u.nom AS destinataire, u.email AS adressedestinataire
FROM remocra.organisme o
	JOIN remocra.utilisateur u ON u.organisme = o.id
	JOIN profil_utilisateur pu ON pu.id = u.profil_utilisateur
WHERE o.id = ${ORGANISME_ID}
UNION
SELECT u.prenom || ' ' || u.nom AS destinataire, u.email AS adressedestinataire
FROM remocra.organisme o
	JOIN remocra.type_organisme tor ON tor.id = o.type_organisme AND tor.code = 'SDIS_GROUPEMENT'
	JOIN remocra.utilisateur u ON u.organisme = o.id
	JOIN remocra.zone_competence zc ON zc.id = o.zone_competence
	JOIN cis ON St_Contains(zc.geometrie, cis.geometrie)
	JOIN profil_utilisateur pu ON pu.id = u.profil_utilisateur
UNION
SELECT u.prenom || ' ' || u.nom AS destinataire, u.email AS adressedestinataire
FROM remocra.organisme o
	JOIN remocra.type_organisme tor ON tor.id = o.type_organisme AND tor.code = 'SDIS_ETAT_MAJOR'
	JOIN remocra.utilisateur u ON u.organisme = o.id
	JOIN remocra.zone_competence zc ON zc.id = o.zone_competence
	JOIN cis ON St_Contains(zc.geometrie, cis.geometrie)
	JOIN profil_utilisateur pu ON pu.id = u.profil_utilisateur</sql>
