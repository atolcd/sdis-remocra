    <entry>
      <name>organisme responsable intersect</name>
      <description/>
      <type>EVAL_TABLE_CONTENT</type>
      <connection>Remocra (Postgis)</connection>
      <schemaname/>
      <tablename/>
      <success_condition>rows_count_greater</success_condition>
      <limit>0</limit>
      <is_custom_sql>Y</is_custom_sql>
      <is_usevars>Y</is_usevars>
      <custom_sql>SELECT
	${COURRIER_ID}::bigint AS document,
	u_sdis.prenom || &apos; &apos; || u_sdis.nom AS nom_destinataire,
	&apos;UTILISATEUR&apos;::text AS type_destinataire,
	u_sdis.id AS id_destinataire,
	u_sdis.email AS email_destinataire
FROM
	-- Collectivité territoriale à informer
	(SELECT
		org.nom,
		zc.geometrie
	FROM
		(SELECT	nom,zone_competence FROM remocra.organisme WHERE id = ${ORGANISME_COLLECTIVITE_ID} AND actif) AS org
		JOIN remocra.zone_competence zc ON(zc.id = org.zone_competence)
	) AS o_ct
	-- Utilisateurs &quot;Affaire suivie par&quot;
	JOIN (SELECT
		u.id,
		u.nom,
		u.prenom,
		u.email,
		zc.geometrie
	FROM
		(SELECT	* FROM remocra.utilisateur WHERE actif AND message_remocra AND profil_utilisateur = ${PROFIL_UTILISATEUR_ID}) AS u
		JOIN remocra.organisme org ON(org.id = u.organisme)
		JOIN remocra.zone_competence zc ON(zc.id = org.zone_competence)
	) AS u_sdis ON (u_sdis.geometrie &amp;&amp; o_ct.geometrie AND St_Intersects(u_sdis.geometrie,o_ct.geometrie))</custom_sql>
      <add_rows_result>Y</add_rows_result>
      <clear_result_rows>Y</clear_result_rows>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>765</xloc>
      <yloc>585</yloc>
      </entry>
