  <step>
    <name>Données hydrant_pibi</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Remocra</connection>
    <sql>SELECT 
  (SELECT CASE thd.code = 'canal_forez' WHEN TRUE THEN 'Vrai' ELSE 'Faux' END) AS canal_forez,
  c.nom AS commune,
  hi.debit AS debit_1bar,
  hi.debit_max AS debit_max_en_m3h,
  substring(c.insee FROM 1 FOR 2) AS dept_num,
  thd.nom AS domaine,
  array_to_string(ARRAY(
                   SELECT t.nom 
                   FROM remocra.hydrant_tournees AS ht 
                   INNER JOIN remocra.tournee AS t ON (t.id=ht.tournees) 
                   WHERE ht.hydrant = h.id ORDER BY t.nom
                 ), ',') AS num_tournee,
  h.complement AS complement_adresse,
  h.observation AS observation,
  hi.pression_dyn AS pression_dynamique_60m3h,
  hi.pression As pression_statique,
  (SELECT sous_type FROM remocra_referentiel.nature_diametre_sig WHERE code_nature = COALESCE(thn.code,'') AND code_diametre = COALESCE(thdi.code,'') LIMIT 1) AS sous_type,
  h.voie AS adresse,
  h.voie2 AS adresse_intersection,
  h.numero AS id_basepei,
  coalesce(h.date_contr, h.date_recep) AS date_controle,
  'SRID=2154;' || st_astext(h.geometrie) AS geometrie,
  (SELECT CASE WHEN (coalesce(h.dispo_terrestre,'')  = 'DISPO' ) THEN 'DI' ELSE 'IN' END) as etat_pei,
  CAST(h.numero_interne AS varchar) AS num_hydrant,
  h.id AS id_remocra,
  thnd.code AS type_deci
FROM remocra.hydrant AS h
INNER JOIN remocra.hydrant_pibi AS hi ON (h.id=hi.id)
LEFT JOIN remocra.commune AS c ON (c.id=h.commune)
LEFT JOIN remocra.type_hydrant_domaine AS thd ON (thd.id=h.domaine)
LEFT JOIN remocra.type_hydrant_nature AS thn ON(thn.id=h.nature)
LEFT JOIN remocra.type_hydrant_diametre AS thdi ON (thdi.id=hi.diametre)
LEFT JOIN remocra.type_hydrant_nature_deci AS thnd ON(thnd.id=h.nature_deci)
WHERE h.date_recep IS NOT NULL;</sql>
