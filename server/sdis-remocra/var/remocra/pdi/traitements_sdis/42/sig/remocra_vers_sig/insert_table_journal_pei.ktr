  <step>
    <name>Données tracabilite.hydrant</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Remocra</connection>
    <sql>SELECT 
  now() as date_message,
  h3.numero as id_basepei,
  h3.date_operation as date_maj,
  (SELECT CASE WHEN (h3.nom_operation = &apos;DELETE&apos;) THEN &apos;Suppression&apos;
  WHEN (h2.nom_operation = &apos;INSERT&apos;) THEN &apos;Création&apos;
  WHEN (h1.date_recep IS NULL AND h3.date_recep IS NOT NULL) THEN &apos;Création&apos;
  ELSE 
    ltrim(
      (SELECT CASE WHEN ( (coalesce(h1.nature,&apos;&apos;) &lt;&gt; coalesce(h3.nature,&apos;&apos;)) OR (coalesce(h1.diametre,&apos;&apos;)  &lt;&gt; coalesce(h3.diametre,&apos;&apos;))) THEN &apos;, Maj du type (nature et&#47;ou diamètre)&apos; ELSE &apos;&apos; END) ||
      (SELECT CASE WHEN ((coalesce(h1.debit,0) &lt;&gt; coalesce(h3.debit,0))) THEN &apos;, Maj de débit&apos; ELSE &apos;&apos; END) ||
      (SELECT CASE WHEN ((coalesce(h1.dispo_terrestre,&apos;&apos;) &lt;&gt; coalesce(h3.dispo_terrestre,&apos;&apos;))) THEN &apos;, Maj disponibilité&apos; ELSE &apos;&apos; END) ||
      (SELECT CASE WHEN (St_Distance(h1.geometrie, h3.geometrie) &gt; 0 OR St_Distance(h1.geometrie, h3.geometrie) IS NULL) THEN &apos;, Maj de position&apos; ELSE &apos;&apos; END) ||
      (SELECT CASE WHEN ((coalesce(h1.capacite,&apos;&apos;) &lt;&gt; coalesce(h3.capacite,&apos;&apos;))) THEN &apos;, Maj de la capacité&apos; ELSE &apos;&apos; END)
    , &apos;, &apos;)::varchar 
  END) as type_maj,
  h3.debit as debit_courant,
  h1.debit as debit_precedent,
  CASE WHEN trim(h3.capacite) SIMILAR TO &apos;[0-9]+&apos; THEN CAST(trim(h3.capacite) AS integer) ELSE null::integer END as capacite_courante,
  CASE WHEN trim(h1.capacite) SIMILAR TO &apos;[0-9]+&apos; THEN CAST(trim(h1.capacite) AS integer) ELSE null::integer END as capacite_precedente,
  h3.nature || coalesce(&apos; &apos; || h3.diametre,&apos;&apos;) as type_pei_courant,
  h1.nature || coalesce(&apos; &apos; || h1.diametre,&apos;&apos;) as type_pei_precedent,
  st_astext(h3.geometrie) as position_courante,
  st_astext(coalesce(h1.geometrie,h3.geometrie)) as position_precedente,
  coalesce((st_astext(h1.geometrie) &lt;&gt; st_astext(h3.geometrie)),false) as maj_position,
  (SELECT CASE WHEN (coalesce(h3.dispo_terrestre, &apos;DISPO&apos;) = &apos;DISPO&apos;) THEN &apos;DI&apos; ELSE &apos;IN&apos; END) as etat_pei_courant,
  (SELECT CASE WHEN (coalesce(h1.dispo_terrestre, &apos;DISPO&apos;) = &apos;DISPO&apos;) THEN &apos;DI&apos; ELSE &apos;IN&apos; END) as etat_pei_precedent
FROM
  ( SELECT 
      hydrant_maj.id_hydrant_maj, 
      (SELECT max(id) FROM tracabilite.hydrant WHERE id_hydrant = hydrant_maj.id_hydrant_maj AND date_operation &lt;= &apos;${date_max}&apos;) as id_action_last_before_last_synchro,
      (SELECT min(id) FROM tracabilite.hydrant WHERE id_hydrant = hydrant_maj.id_hydrant_maj AND date_operation &gt; &apos;${date_max}&apos;) as id_action_first_after_last_synchro,
      (SELECT max(id) FROM tracabilite.hydrant WHERE id_hydrant = hydrant_maj.id_hydrant_maj AND date_operation &gt; &apos;${date_max}&apos;) as id_action_last
    FROM 
      (SELECT DISTINCT (id_hydrant) as id_hydrant_maj FROM tracabilite.hydrant  WHERE date_operation &gt; &apos;${date_max}&apos; ORDER BY id_hydrant) as hydrant_maj
  ) as hydrant_idaction
  LEFT JOIN tracabilite.hydrant as h1 ON (h1.id = id_action_last_before_last_synchro)
  INNER JOIN tracabilite.hydrant as h2 ON (h2.id = id_action_first_after_last_synchro)
  INNER JOIN tracabilite.hydrant as h3 ON (h3.id = id_action_last)
WHERE 
   (NOT (h3.nom_operation = &apos;DELETE&apos; AND h2.nom_operation = &apos;INSERT&apos;))
AND h3.date_recep IS NOT NULL 
AND 
  ((St_Distance(h1.geometrie, h3.geometrie) &gt; 0 OR St_Distance(h1.geometrie, h3.geometrie) IS NULL)
  OR ((coalesce(h1.debit,0) &lt;&gt; coalesce(h3.debit,0))) 
  OR ((coalesce(h1.nature,&apos;&apos;) &lt;&gt; coalesce(h3.nature,&apos;&apos;)))
  OR ((coalesce(h1.diametre,&apos;&apos;) &lt;&gt; coalesce(h3.diametre,&apos;&apos;)))
  OR ((coalesce(h1.dispo_terrestre,&apos;&apos;) &lt;&gt; coalesce(h3.dispo_terrestre,&apos;&apos;)))
  OR ((coalesce(h1.capacite,&apos;&apos;) &lt;&gt; coalesce(h3.capacite,&apos;&apos;))) 
  OR ((SELECT CASE WHEN (coalesce(h1.dispo_terrestre, &apos;DISPO&apos;) = &apos;DISPO&apos;) THEN &apos;DI&apos; ELSE &apos;IN&apos; END) &lt;&gt; (SELECT CASE WHEN (coalesce(h3.dispo_terrestre, &apos;DISPO&apos;) = &apos;DISPO&apos;) THEN &apos;DI&apos; ELSE &apos;IN&apos; END))
  OR (h3.nom_operation = &apos;DELETE&apos;)
  OR (h2.nom_operation = &apos;INSERT&apos;)
  )
;</sql>
