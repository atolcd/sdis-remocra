  <step>
    <name>Données des PEI</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Remocra (Postgis)</connection>
    <sql>SELECT
h.id,
h.geometrie,
h.numero AS numero_complet,
h.numero_interne,
h.voie AS nom_voie,
TRIM(  case when h.en_face = TRUE then 'F' else ' ' end || ' '
|| COALESCE(h.numero_voie::text, '0') || ' '
|| COALESCE(h.suffixe_voie, '')) as numero,
nd.code AS nature_deci,
h.code AS type_hydrant,
c.nom AS nom_commune,
nh.nom AS nature_hydrant_nom,
CASE
WHEN h.code = 'PENA' AND nh.code LIKE 'CI_%' THEN 'CITERNE'
WHEN h.code = 'PENA' AND nh.code NOT LIKE 'CI_%' THEN 'PA'
ELSE nh.code
END AS nature_hydrant,
replace(ROUND((pibi.diametre_canalisation/100::numeric),1)::text,'.0','') AS diametre_canalisation,
pena.capacite,
CASE WHEN COALESCE(asp.hauteur,false) THEN '+3' ELSE NULL END AS hauteur_asp, -- Aspiration de plus de 3 m
CASE
WHEN h.code = 'PENA' AND nh.code LIKE 'CI_%' THEN 'CITERNE'
WHEN h.code = 'PENA' AND nh.code NOT LIKE 'CI_%' THEN 'PA'
ELSE nh.code
END -- Nature du PEI
|| COALESCE((CASE WHEN (SELECT id from remocra.type_hydrant_anomalie where code ='NON_NORMALISE_NC')
 IN (SELECT anomalies from remocra.hydrant_anomalies where hydrant = h.id) AND h.dispo_terrestre = 'DISPO' THEN('_AUTRE') ELSE '' END --Si on a l'anomalie NON_NORMALISE_NC
|| 
CASE WHEN dispo_terrestre='DISPO' THEN ('_' || replace(nd.code,'CONVENTIONNE','PUBLIC') || '_') ELSE '_' END -- Si dispo, ajoute le type DECI (CONVENTIONNE => PUBLIC)
|| dispo_terrestre -- Disponibilité
|| CASE WHEN thn.code='INFRA' THEN ('_' || thn.code) ELSE ( -- SI VP ou SUPRA, ajoute le caractere optionnel debit renforcé
CASE WHEN debit_renforce THEN '_DR' ELSE ( -- SI _GD non renseigné , ajoute diametre  alimentation diametre canalisation
CASE WHEN nh.code = 'BI' AND pibi_diam.code= 'DIAM100' AND pibi.jumele IS NOT NULL THEN '_GD' ELSE 
(CASE WHEN nh.code = 'PI' AND pibi_diam.code= 'DIAM150' THEN '_GD' ELSE '' END ) END)END
) END || '.png'),'_INCONNU.png') as img
FROM
remocra.hydrant h
LEFT JOIN remocra.type_hydrant_nature_deci nd ON (nd.id = h.nature_deci)
LEFT JOIN remocra.type_hydrant_nature nh ON (nh.id = h.nature)
LEFT JOIN remocra.type_hydrant_niveau thn ON (thn.id = h.niveau)
LEFT JOIN remocra.commune c ON (c.id = h.commune)
LEFT JOIN remocra.hydrant_pibi pibi ON (pibi.id = h.id)
LEFT JOIN remocra.type_hydrant_diametre pibi_diam ON (pibi_diam.id = pibi.diametre)
LEFT JOIN remocra.type_reseau_alimentation tra ON (tra.id = pibi.type_reseau_alimentation)
LEFT JOIN remocra.hydrant_pena pena ON (pena.id = h.id)
LEFT JOIN (SELECT pena,count(*)>0 AS hauteur FROM remocra.hydrant_aspiration WHERE hauteur GROUP BY pena) asp ON (asp.pena = pena.id)
</sql>
