        <class_source>boolean french;
String fieldnameField;
String commentsField;
String lengthField;
String precisionField;
String originField;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
    //
    Object[] r = getRow();

    // If the row object is null, we are done processing.
    //
    if (r == null) {
      setOutputDone();
      return false;
    }

    // Let's look up parameters only once for performance reason.
    //
    if (first) {
      french = false;
      try {
        get(Fields.In, "Fieldname").getString(r);
      }
      catch (Exception e){
        french = true;
        fieldnameField = "Nom champ";
        commentsField = "Description";
        lengthField = "Longueur";
        precisionField = "Précision";
        originField = "Etape source";
      }
      first=false;
    }
 
    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    //
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
 
    if(french) {
      String fieldname = get(Fields.In, fieldnameField).getString(r);
      String comments = get(Fields.In, commentsField).getString(r);
      String length = get(Fields.In, lengthField).getString(r);
      String precision = get(Fields.In, precisionField).getString(r);
      String origin = get(Fields.In, originField).getString(r);

      // Set the value in the output field
      //
      get(Fields.Out, "Fieldname").setValue(outputRow, fieldname);
      get(Fields.Out, "Comments").setValue(outputRow, comments);
      get(Fields.Out, "Length").setValue(outputRow, length);
      get(Fields.Out, "Precision").setValue(outputRow, precision);
      get(Fields.Out, "Origin").setValue(outputRow, origin);
    }
 
    // putRow will send the row on to the default output hop.
    //
    putRow(data.outputRowMeta, outputRow);
 
    return true;
}</class_source>
