    <name>Input pibi</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Remocra</connection>
    <sql>SELECT 
	h.numero AS num,
	n.nom AS nature,
	h.dispo_terrestre AS dispo_pei,
	aut.nom AS autorite_police,
	thnd.nom AS typedeci,
	CASE
		WHEN h.nature_deci IN (SELECT id FROM remocra.type_hydrant_nature_deci WHERE code = 'PUBLIC' OR code = 'CONVENTIONNE') THEN gest_pub.nom
		WHEN h.nature_deci = (SELECT id FROM remocra.type_hydrant_nature_deci WHERE code = 'PRIVE') THEN gest_priv.nom
	END AS gest_pte,
	s.nom AS site,
	c.insee As com_insee,
	c.nom AS com_nom,
	d.nom AS domaine,
	h.numero_voie AS num_v,
	h.suffixe_voie AS num_v_su,
	CASE WHEN h.en_face THEN 1 ELSE 0 END AS en_face,
	thn.nom AS etage,
	h.voie,
	h.voie2,
	h.lieu_dit,
	h.complement AS complt,
	diam.nom AS diametre,
	CASE WHEN hp.dispositif_inviolabilite THEN 1 ELSE 0 END AS inviolabilite,
	CASE WHEN hp.renversable THEN 1 ELSE 0 END AS choc,
	jum.numero AS jumele,
	m.nom AS marque,
	model.nom AS modele,
	h.annee_fabrication AS annee_fa,
	gest_res.nom AS gest_res,
	tra.nom AS type_res,
	CASE WHEN hp.debit_renforce THEN 1 ELSE 0 END AS debit_renforce,
	trc.nom AS typecana,
	diametre_canalisation AS diam_cana,
	hr.nom AS reservoir_nom,
	hr.capacite AS reservoir_capa,
	CASE WHEN hp.surpresse THEN 1 ELSE 0 END AS reseau_surpre,
	CASE WHEN hp.additive THEN 1 ELSE 0 END AS reseau_additi,
	hp.debit,
	hp.pression_dyn AS pres_dyn,
	hp.debit_max,
	hp.pression_dyn_deb AS presdyde,
	hp.pression,
	h.observation AS observat,
	COALESCE(h.date_modification, '1970-01-01'::date) AS date_maj,
	h.geometrie 
FROM remocra.hydrant AS h 
	INNER JOIN remocra.hydrant_pibi AS hp ON (hp.id=h.id)
	LEFT JOIN remocra.commune AS c ON (h.commune=c.id)
	LEFT JOIN remocra.type_hydrant_domaine AS d ON (h.domaine=d.id)
	LEFT JOIN remocra.type_hydrant_nature AS n ON (h.nature=n.id)
	LEFT JOIN remocra.type_hydrant_diametre AS diam ON (hp.diametre=diam.id)
	LEFT JOIN remocra.type_hydrant_marque AS m ON (hp.marque=m.id)
	LEFT JOIN remocra.type_hydrant_modele AS model ON (hp.modele=model.id)
	LEFT JOIN remocra.organisme AS aut ON aut.id = h.autorite_deci
	LEFT JOIN remocra.type_hydrant_nature_deci AS thnd ON thnd.id = h.nature_deci
	LEFT JOIN remocra.organisme gest_pub ON gest_pub.id = h.gestionnaire
	LEFT JOIN remocra.gestionnaire gest_priv ON gest_priv.id = h.gestionnaire
	LEFT JOIN remocra.site s ON s.id = h.site
	LEFT JOIN remocra.type_hydrant_niveau thn ON thn.id = h.niveau
	LEFT JOIN remocra.hydrant jum ON jum.id = hp.jumele
	LEFT JOIN remocra.organisme gest_res ON gest_res.id = hp.service_eaux
	LEFT JOIN remocra.type_reseau_alimentation tra ON tra.id = hp.type_reseau_alimentation
	LEFT JOIN remocra.type_reseau_canalisation trc ON trc.id = hp.type_reseau_canalisation
	LEFT JOIN remocra.hydrant_reservoir hr ON hr.id = hp.reservoir;</sql>
