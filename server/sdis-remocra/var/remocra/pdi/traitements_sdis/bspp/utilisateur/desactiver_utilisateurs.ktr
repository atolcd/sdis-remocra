    <quoteString>N</quoteString>
    <sql>WITH RECURSIVE org(id) AS (
	SELECT id FROM remocra.organisme WHERE code = 'BSPP'
	UNION ALL
	SELECT o.id
	FROM org 
		JOIN remocra.organisme o ON o.organisme_parent = org.id
),utilisateur_a_deact AS (
	SELECT u.id
	FROM remocra.utilisateur u
	WHERE u.organisme IN (SELECT id FROM org)
)
UPDATE remocra.utilisateur
SET actif = FALSE 
WHERE id IN (SELECT id FROM utilisateur_a_deact);</sql>
