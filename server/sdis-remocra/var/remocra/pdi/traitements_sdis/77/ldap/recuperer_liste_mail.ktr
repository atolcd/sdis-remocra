    <rowlimit>0</rowlimit>
    <sql>SELECT email AS destinataire_email, u.prenom || &apos; &apos; || u.nom AS destinataire
FROM remocra.utilisateur u
INNER JOIN remocra.profil_utilisateur pu ON pu.id = u.profil_utilisateur AND pu.code = ?
WHERE u.email &lt;&gt; &apos;&apos; AND u.message_remocra AND u.actif</sql>
