    <entry>
      <name>Consolidation des infos de traçabilité PEI</name>
      <description/>
      <type>SQL</type>
      <sql>INSERT INTO remocra_sgo.hydrant_tracabilite (date_message, num, na_pt_code, dep_num, cod_insee, etat_pt_eau_cod, cs_code, statut, adr_1, adr_2, cp, ville, num_voie, coord_x, coord_y, debit, capacite, diam_canal, cod_valid, date_maj_remocra, type_maj, carroyage)
SELECT
	now() AS date_message,
	substring(h.numero from &apos;....$&apos;)AS num,
	sgo_n.code_nature_sgo AS na_pt_code,
	substr(h.insee,1,2) AS dep_num,
	substr(h.insee,3,3) AS cod_insee,
	CASE
		WHEN h.dispo_terrestre = &apos;DISPO&apos; THEN &apos;DI&apos;
		WHEN h.dispo_terrestre = &apos;NON_CONFORME&apos; THEN &apos;DP&apos;
		WHEN h.dispo_terrestre = &apos;INDISPO&apos; THEN &apos;IN&apos;
		ELSE &apos;?&apos;
	END AS eta_pt_eau_cod,
	cis.code AS cs_code,
	COALESCE(thd.code,&apos;&apos;) AS statut,
	substr(h.voie,1,32) AS adr_1,
	substr(h.lieu_dit,1,32) AS adr_2,	
	null::character varying AS cp,
	substr(h.commune,1,26) AS ville,
	null::character varying AS num_voie,
	st_x(h.geometrie) AS coord_x,
	st_y(h.geometrie) AS coord_y,
	h.debit AS debit,
	null::double precision	AS capacite,
	null::double precision	AS diam_canal,
	null::character varying AS cod_valid,
	h.date_operation AS date_maj_remocra,
	h.nom_operation AS type_maj,
	carroy.idcarre200 AS carroyage
FROM
	tracabilite.hydrant h
	JOIN remocra.type_hydrant_nature n ON(n.nom = h.nature)
	LEFT JOIN remocra.type_hydrant_diametre d ON(d.nom = h.diametre)
	LEFT JOIN remocra.type_hydrant_positionnement p ON(p.nom = h.positionnement)
	LEFT JOIN remocra.type_hydrant_domaine thd ON thd.nom = h.domaine
	LEFT JOIN (SELECT idcarre200, shape FROM remocra_referentiel.carre_200) carroy ON ST_Contains(carroy.shape, h.geometrie)
	JOIN remocra_sgo.hydrant_nature sgo_n ON(sgo_n.code_nature_remocra = replace(n.code || &apos;_&apos; ||  COALESCE(d.code,&apos;_&apos;) || &apos;_&apos; || COALESCE(p.code,&apos;_&apos;),&apos;__&apos;,&apos;&apos;))
	JOIN (SELECT
			org.code,
			zc.geometrie
		FROM
			remocra.organisme org
			JOIN (SELECT id FROM remocra.type_organisme WHERE code = &apos;CIS_OPERATIONNEL&apos;) torg ON (torg.id = org.type_organisme)
			JOIN remocra.zone_competence zc ON(zc.id =  org.zone_competence)
	) AS cis ON (cis.geometrie &amp;&amp; h.geometrie AND st_contains(cis.geometrie, h.geometrie))
WHERE
	h.date_operation &gt; (SELECT COALESCE(max(date_message),&apos;1900-01-01&apos;::timestamp without time zone) cis FROM remocra_sgo.hydrant_tracabilite)
ORDER BY
	h.date_operation DESC;
</sql>
