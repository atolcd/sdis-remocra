    <connection>REMOCRA_POSTGIS</connection>
    <sql>SELECT DISTINCT ON (h.id) h.id, substr(substr(h.numero,1,5) || &apos;_&apos; || substr(h.numero,6,10),1,10) AS IdPtEau, h.numero_interne AS NUM_PEI,
