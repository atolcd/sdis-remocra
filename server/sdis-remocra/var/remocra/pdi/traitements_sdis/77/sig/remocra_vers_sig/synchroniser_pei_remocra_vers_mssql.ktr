   substr(carroy.idcarre200,1,10) AS CARROYAGE, h.dispo_terrestre, to_char(traca.date_operation, &apos;DD&#47;MM&#47;YYYY&apos;) AS created_date, 
   traca.auteur_modification AS created_user, to_char(h.date_modification, &apos;DD&#47;MM&#47;YYYY&apos;) AS last_edited_date,
   COALESCE(u.nom,&apos; &apos;) || &apos; &apos; || COALESCE(u.prenom, &apos; &apos;) AS last_edited_user, to_char(now(), &apos;DD&#47;MM&#47;YYYY&apos;) AS Date_Extraction,
   COALESCE ((SELECT this.nom FROM remocra.hydrant_indispo_temporaire as hit INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON (hit.id=hith.indisponibilite) INNER JOIN remocra.type_hydrant_indispo_statut this ON (this.id=hit.statut) WHERE hith.hydrant=h.id ORDER BY date_fin DESC, statut LIMIT 1), &apos;&apos;) as ETAT_TMP_PEI,
   &apos;&apos; as DATES_TMP_PREVU,
   (SELECT to_char(min(date_debut), &apos;DD&#47;MM&#47;YYYY&apos;) FROM remocra.hydrant_indispo_temporaire as hit INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON (hit.id=hith.indisponibilite) WHERE hith.hydrant=h.id AND hit.statut = 1) as DATE_DEB_TMP,
   (SELECT COUNT(0) FROM remocra.hydrant_indispo_temporaire as hit INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON (hit.id=hith.indisponibilite) WHERE hith.hydrant=h.id AND hit.statut = 1) as NBR_TMP_ACTIF   
FROM remocra.hydrant h
   INNER JOIN remocra.hydrant_pibi hpi ON hpi.id = h.id
   LEFT JOIN remocra.commune c ON c.id = h.commune
   LEFT JOIN remocra.type_hydrant_domaine thdo ON thdo.id = h.domaine
   LEFT JOIN remocra.type_hydrant_nature thn ON thn.id = h.nature
   LEFT JOIN remocra.type_hydrant_diametre thdi ON thdi.id = hpi.diametre
   LEFT JOIN remocra_sgo.hydrant_nature hn ON hn.code_nature_remocra = COALESCE(thn.code, &apos; &apos;) || &apos;_&apos; || COALESCE(thdi.code, &apos;DIAM_IND&apos;)
   LEFT JOIN (SELECT id_hydrant, date_operation, auteur_modification FROM tracabilite.hydrant WHERE nom_operation = &apos;INSERT&apos;) AS traca ON traca.id_hydrant = h.id
   LEFT JOIN (SELECT idcarre200, shape FROM remocra_referentiel.carre_200) carroy ON ST_Contains(carroy.shape, h.geometrie)
   LEFT JOIN remocra.utilisateur u ON u.id = h.utilisateur_modification

UNION

SELECT DISTINCT ON (h.id) h.id, substr(substr(h.numero,1,5) || &apos;_&apos; || substr(h.numero,6,10),1,10) AS IdPtEau, h.numero_interne AS NUM_PEI,
   &apos;geometry::STGeomFromText(&apos;&apos;&apos;|| st_astext(h.geometrie) ||&apos;&apos;&apos;,&apos;|| st_srid(h.geometrie) ||&apos;)&apos; AS shape, substr(hn.code_nature_sgo,1,80) AS NATURE,
   coalesce(substr(thdo.code,1,20), &apos;U&apos;) AS STATUT, substr(c.insee,1,5) AS CODE_INSEE, c.nom AS COMMUNE,
   substr(carroy.idcarre200,1,10) AS CARROYAGE, h.dispo_terrestre, to_char(traca.date_operation, &apos;DD&#47;MM&#47;YYYY&apos;) AS created_date, 
   traca.auteur_modification AS created_user, to_char(h.date_modification, &apos;DD&#47;MM&#47;YYYY&apos;) AS last_edited_date,
   COALESCE(u.nom,&apos; &apos;) || &apos; &apos; || COALESCE(u.prenom, &apos; &apos;) AS last_edited_user, to_char(now(), &apos;DD&#47;MM&#47;YYYY&apos;) AS Date_Extraction,
   COALESCE ((SELECT this.nom FROM remocra.hydrant_indispo_temporaire as hit INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON (hit.id=hith.indisponibilite) INNER JOIN remocra.type_hydrant_indispo_statut this ON (this.id=hit.statut) WHERE hith.hydrant=h.id ORDER BY date_fin DESC, statut LIMIT 1), &apos;&apos;) as ETAT_TMP_PEI,
   &apos;&apos; as DATES_TMP_PREVU,
   (SELECT to_char(min(date_debut), &apos;DD&#47;MM&#47;YYYY&apos;) FROM remocra.hydrant_indispo_temporaire as hit INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON (hit.id=hith.indisponibilite) WHERE hith.hydrant=h.id AND hit.statut = 1) as DATE_DEB_TMP,
   (SELECT COUNT(0) FROM remocra.hydrant_indispo_temporaire as hit INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON (hit.id=hith.indisponibilite) WHERE hith.hydrant=h.id AND hit.statut = 1) as NBR_TMP_ACTIF
FROM remocra.hydrant h
   INNER JOIN remocra.hydrant_pena hpe ON hpe.id = h.id
   LEFT JOIN remocra.commune c ON c.id = h.commune
   LEFT JOIN remocra.type_hydrant_domaine thdo ON thdo.id = h.domaine
   LEFT JOIN remocra.type_hydrant_nature thn ON thn.id = h.nature
   LEFT JOIN remocra.type_hydrant_positionnement thp ON thp.id = hpe.positionnement
   LEFT JOIN remocra_sgo.hydrant_nature hn ON hn.code_nature_remocra = COALESCE(thn.code, &apos; &apos;)
   LEFT JOIN (SELECT id_hydrant, date_operation, auteur_modification FROM tracabilite.hydrant WHERE nom_operation = &apos;INSERT&apos;) AS traca ON traca.id_hydrant = h.id
   LEFT JOIN (SELECT idcarre200, shape FROM remocra_referentiel.carre_200) carroy ON ST_Contains(carroy.shape, h.geometrie)
   LEFT JOIN remocra.utilisateur u ON u.id = h.utilisateur_modification;</sql>
