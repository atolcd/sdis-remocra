    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import javax.mail.*;
import javax.mail.internet.InternetAddress;
import javax.mail.internet.MimeMessage;
import java.util.Properties;

Session session;

String smtpHost; // Host du STMP
String smtpPort; // Port du STMP
String smtpUser; // User name du SMTP
String smtpPassword;//User password du SMTP
String smtpReponseMail; //Adresse mél de réponse
String smtpARMail;	// Adresse mél de retour des accusés des récéption
String smtpErreurMail; //Adresse mél de retour des erreurs de délivrance

String messageID; // Identifiant de message généré par rémocra
String messageExpediteurNom; // Nom de l'expéditeur
String messageExpediteurMail; // Adresse mél de l'expéditeur
String messageDestinataireNom; // Nom du destinataire
String messageDestinataireMail; // Adresse mél du destinataire
String messageObjet; // Objet du mél
String messageCorps; // Corps du mél en HTML
Boolean messageAR;  // Demander accusé de réception

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException{

	Object[] r = getRow();

	if (r == null) {
		setOutputDone();
		return false;
	}

	if (first) {

		//Récupération des paramètres du service SMTP
		smtpHost = get(Fields.In, "smtp_url").getString(r);
		smtpPort = get(Fields.In, "smtp_port").getString(r);
		smtpUser = get(Fields.In, "smtp_user").getString(r);
		smtpPassword = get(Fields.In, "smtp_password").getString(r);
		smtpErreurMail = get(Fields.In, "erreur_delivrance_mail").getString(r);

		Properties properties = new Properties();
		properties.put("mail.smtp.host", smtpHost);
		properties.put("mail.smtp.port", smtpPort);
		properties.put("mail.smtp.connectiontimeout", "1500");
    	properties.put("mail.smtp.timeout", "1500");

		//Adresse mél de reception des erreurs
		properties.put("mail.smtp.from", smtpErreurMail);

		//Création d'une session
		Authenticator authenticator = new javax.mail.Authenticator() {  
			protected PasswordAuthentication getPasswordAuthentication() {  
				return new PasswordAuthentication(smtpUser,smtpPassword);  
			}
		};

		session = Session.getInstance(properties, authenticator);

	    first=false;
	}

    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

	//Récupération des informations sur le message à envoyer
	messageID = get(Fields.In, "code").getString(r);
	messageExpediteurNom = get(Fields.In, "expediteur").getString(r);
	messageExpediteurMail = get(Fields.In, "expediteur_email").getString(r);
	messageDestinataireNom = get(Fields.In, "destinataire").getString(r);
	messageDestinataireMail = get(Fields.In, "destinataire_email").getString(r);
	messageObjet = get(Fields.In, "objet").getString(r);
	messageCorps = get(Fields.In, "corps").getString(r);
	messageAR = get(Fields.In, "accuser_reception").getBoolean(r);

	smtpReponseMail = get(Fields.In, "reponse_mail").getString(r);
	smtpARMail = get(Fields.In, "accuse_reception_mail").getString(r);

	try{

		//Nouveau message avec ID personnalisé
		MimeMessage message = new MimeMessage(session) {
			protected void updateMessageID() throws MessagingException {
		        setHeader("Message-ID", messageID);
		    }
		};

		//Accusé de récéption du mel si démandé pour le mail et adresse non nulle
		if(messageAR
			&amp;&amp; smtpARMail != null
			&amp;&amp; !smtpARMail.isEmpty())
		{
			message.setHeader("Disposition-Notification-To", smtpARMail);
	   		message.setHeader("Return-Receipt-To", smtpARMail);
		};

		//Expéditeur
     	message.setFrom(
			new InternetAddress(messageExpediteurMail, messageExpediteurNom)
		);

		//Destinataire  
     	message.addRecipient(
			Message.RecipientType.TO,
			new InternetAddress(messageDestinataireMail, messageDestinataireNom)
		);

		//Adresse de réponse
		message.setReplyTo(new InternetAddress[]{new InternetAddress(smtpReponseMail)});


    	//Objet et corps HTML en UTF8
		message.setSubject(messageObjet,"UTF-8");
		message.setContent(messageCorps,"text/html; charset=UTF-8");
	      	
		//Envoyer le message
		Transport.send(message);
		
	}catch (Exception e) {
			new KettleException(e.toString());
	}

    putRow(data.outputRowMeta, outputRow);

	return true;
}</class_source>
      </definition>
