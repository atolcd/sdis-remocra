    <entry>
      <name>Bascule auto</name>
      <description/>
      <type>SQL</type>
      <sql>&#x2f;&#x2a;&#x2a;&#x2a; Insertion de l&#x27;anomalies INDISPONIBILITE_TEMP pour les PEI concern&#xe9;s par une bascule auto &#x2a;&#x2a;&#x2a;&#x2a;&#x2f;&#xa;INSERT INTO remocra.hydrant_anomalies &#x28;hydrant, anomalies&#x29;&#xa;SELECT DISTINCT h.id AS hydrant, tha.id AS anomalies&#xa;FROM remocra.hydrant_indispo_temporaire it&#xa;	INNER JOIN remocra.type_hydrant_indispo_statut tis ON tis.id &#x3d; it.statut AND tis.CODE &#x3d; &#x27;PLANIFIE&#x27;&#xa;	INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON hith.indisponibilite &#x3d; it.id&#xa;	INNER JOIN remocra.hydrant h ON h.id &#x3d; hith.hydrant&#xa;	LEFT JOIN &#x28;&#xa;		SELECT ha.hydrant &#xa;		FROM remocra.hydrant_anomalies ha &#xa;		JOIN remocra.type_hydrant_anomalie tha ON tha.id &#x3d; ha.anomalies AND tha.code &#x3d; &#x27;INDISPONIBILITE_TEMP&#x27;&#xa;	&#x29; indispo ON indispo.hydrant &#x3d; h.id&#xa;	JOIN remocra.type_hydrant_anomalie tha ON 1&#x3d;1 AND tha.code &#x3d; &#x27;INDISPONIBILITE_TEMP&#x27;&#xa;WHERE it.bascule_auto_indispo AND&#xa;	indispo.hydrant IS NULL AND&#xa;	it.date_debut &#x3c;&#x3d; current_timestamp&#x3b;&#xa;&#xa;&#x2f;&#x2a;&#x2a;&#x2a; Bascule de l&#x27;indispo temporaire &#xe0; en cours &#x2a;&#x2a;&#x2a;&#x2a;&#x2f;&#xa;UPDATE remocra.hydrant_indispo_temporaire&#xa;SET statut &#x3d; &#x28;SELECT id FROM remocra.type_hydrant_indispo_statut WHERE code &#x3d; &#x27;EN_COURS&#x27;&#x29;&#xa;WHERE id IN &#x28;&#xa;	SELECT DISTINCT it.id&#xa;	FROM remocra.hydrant_indispo_temporaire it&#xa;		INNER JOIN remocra.type_hydrant_indispo_statut tis ON tis.id &#x3d; it.statut AND tis.CODE &#x3d; &#x27;PLANIFIE&#x27;&#xa;	WHERE it.bascule_auto_indispo AND&#xa;		it.date_debut &#x3c;&#x3d; current_timestamp&#xa;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2a;&#x2a;&#x2a; Suppression de l&#x27;anomalies INDISPONIBILITE_TEMP pour les PEI concern&#xe9;s par une bascule auto &#x2a;&#x2a;&#x2a;&#x2a;&#x2f;&#xa;DELETE FROM remocra.hydrant_anomalies&#xa;WHERE hydrant IN &#x28;&#xa;	SELECT DISTINCT h.id AS hydrant&#xa;	FROM remocra.hydrant_indispo_temporaire it&#xa;		INNER JOIN remocra.type_hydrant_indispo_statut tis ON tis.id &#x3d; it.statut AND tis.code &#x3d; &#x27;EN_COURS&#x27;&#xa;		INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON hith.indisponibilite &#x3d; it.id&#xa;		INNER JOIN remocra.hydrant h ON h.id &#x3d; hith.hydrant&#xa;		LEFT JOIN &#x28;&#xa;			SELECT h.id AS hydrant, COUNT&#x28;it.id&#x29; as nb_indispo&#xa;			FROM remocra.hydrant h&#xa;				JOIN remocra.hydrant_indispo_temporaire_hydrant ith ON ith.hydrant &#x3d; h.id&#xa;				JOIN remocra.hydrant_indispo_temporaire it ON it.id &#x3d; ith.indisponibilite&#xa;				JOIN remocra.type_hydrant_indispo_statut tis ON tis.id &#x3d; it.statut AND tis.code &#x3d; &#x27;EN_COURS&#x27;&#xa;			WHERE it.date_debut &#x3c;&#x3d; current_timestamp AND&#xa;				it.date_fin &#x3e; current_timestamp&#xa;			GROUP BY h.id&#xa;		&#x29; AS count_indispo ON count_indispo.hydrant &#x3d; h.id&#xa;	WHERE it.bascule_auto_dispo AND&#xa;		date_fin &#x3c;&#x3d; current_timestamp AND &#xa;		count_indispo.nb_indispo IS NULL&#xa;	&#x29; AND&#xa;	anomalies &#x3d; &#x28;SELECT id FROM remocra.type_hydrant_anomalie WHERE code &#x3d; &#x27;INDISPONIBILITE_TEMP&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2a;&#x2a;&#x2a; Bascule de l&#x27;indispo temporaire &#xe0; termin&#xe9;e &#x2a;&#x2a;&#x2a;&#x2a;&#x2f;&#xa;UPDATE remocra.hydrant_indispo_temporaire&#xa;SET statut &#x3d; &#x28;SELECT id FROM remocra.type_hydrant_indispo_statut WHERE code &#x3d; &#x27;TERMINE&#x27;&#x29;&#xa;WHERE id IN &#x28;&#xa;	SELECT DISTINCT it.id&#xa;	FROM remocra.hydrant_indispo_temporaire it&#xa;		INNER JOIN remocra.type_hydrant_indispo_statut tis ON tis.id &#x3d; it.statut AND tis.code &#x3d; &#x27;EN_COURS&#x27;&#xa;	WHERE it.bascule_auto_dispo AND&#xa;		it.date_fin &#x3c;&#x3d; current_timestamp&#xa;&#x29;&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>T</sendOneStatement>
      <connection>Remocra &#x28;Postgis&#x29;</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
