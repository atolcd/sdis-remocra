    </entry>
    <entry>
      <name>mise_a_jour_debut_indispo_notifie</name>
      <description />
      <type>SQL</type>
      <sql>UPDATE remocra.hydrant_indispo_temporaire
SET date_rappel_debut = current_timestamp
WHERE id IN 
	(
		SELECT DISTINCT it.id
		FROM remocra.hydrant_indispo_temporaire it
		INNER JOIN remocra.type_hydrant_indispo_statut tis ON tis.id = it.statut AND tis.CODE = 'PLANIFIE'
		WHERE date_rappel_debut IS NULL AND
			mel_avant_indispo AND
			date_debut >= current_timestamp AND
			date_debut &lt;= current_timestamp + interval '${PDI_DELTA_NOTIF_INDISPO_DEBUT} hour'
	)</sql>
