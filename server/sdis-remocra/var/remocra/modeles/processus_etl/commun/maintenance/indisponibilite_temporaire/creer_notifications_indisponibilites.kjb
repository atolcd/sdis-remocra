    <entry>
      <name>verification_notif_debut</name>
      <description />
      <type>EVAL_TABLE_CONTENT</type>
      <connection>Remocra (Postgis)</connection>
      <schemaname />
      <tablename />
      <success_condition>rows_count_greater</success_condition>
      <limit>0</limit>
      <is_custom_sql>Y</is_custom_sql>
      <is_usevars>Y</is_usevars>
      <custom_sql>SELECT DISTINCT h.id AS hydrant
FROM remocra.hydrant_indispo_temporaire it
INNER JOIN remocra.type_hydrant_indispo_statut tis ON tis.id = it.statut AND tis.CODE = 'PLANIFIE'
INNER JOIN remocra.hydrant_indispo_temporaire_hydrant hith ON hith.indisponibilite = it.id
INNER JOIN remocra.hydrant h ON h.id = hith.hydrant
WHERE date_rappel_debut IS NULL AND
	mel_avant_indispo AND
	date_debut >= current_timestamp AND
	date_debut &lt;= current_timestamp + interval '${PDI_DELTA_NOTIF_INDISPO_DEBUT} hour'</custom_sql>
      <add_rows_result>Y</add_rows_result>
      <clear_result_rows>Y</clear_result_rows>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>480</xloc>
