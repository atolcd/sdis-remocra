  <step>
    <name>XML Historique de crise</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Remocra</connection>
    <sql>SELECT
	xmlelement(name "historiqueCrise",
		xmlattributes(
				to_char(to_timestamp('${DATE_HEURE_DEBUT}', 'YYYY-MM-DD HH24:MI:SS') ,'DD/MM/YYYY') AS "dateDebutExport",
				to_char(to_timestamp('${DATE_HEURE_DEBUT}', 'YYYY-MM-DD HH24:MI:SS') ,'HH24:MI') AS "heureDebutExport",
				to_char(coalesce(to_timestamp('${DATE_HEURE_FIN}', 'YYYY-MM-DD HH24:MI:SS'),now()),'DD/MM/YYYY') AS "dateFinExport",
				to_char(coalesce(to_timestamp('${DATE_HEURE_FIN}', 'YYYY-MM-DD HH24:MI:SS'),now()),'HH24:MI') AS "heureFinExport"
		),
		xmlelement(
			name crise,
			xmlattributes(
				to_char(activation,'DD/MM/YYYY') AS "dateActivation",
				to_char(activation,'HH24:MI') AS "heureActivation",
				to_char(coalesce(cloture,now()),'DD/MM/YYYY') AS "dateCloture",
				to_char(coalesce(cloture,now()),'HH24:MI') AS "heureCloture"
			),
			xmlelement(name nom, c.nom),
			xmlelement(name description, c.description)
			
		),
		cs.messages
	) AS historique_crise_xml
FROM
	(SELECT * FROM remocra.crise WHERE id = ${CRISE_ID}) c
	JOIN(
		-- Messages par crise
		SELECT
			crise,
			xmlagg(messages_du_jour) AS messages
		FROM
		-- Messages par jour
		(SELECT
			crise,
			xmlelement(name messages, xmlattributes(jour_message AS "dateCreation"),xmlagg(message)) AS messages_du_jour
		FROM
			(SELECT
				crise,
				cs.creation::date AS date_message,
				to_char(cs.creation,'DD/MM/YYYY') AS jour_message,
				xmlelement(name message,
					xmlattributes(to_char(cs.creation,'HH24:MI') AS "heureCreation"),
					xmlelement(name objet,cs.objet),
					xmlelement(name corps,('&lt;ligne>'|| regexp_replace(cs.message, E'[\\n\\r]+','&lt;/ligne>&lt;ligne>', 'g') || '&lt;/ligne>')::xml),
					xml_doc
				) AS message
				
			FROM
				remocra.crise_suivi cs
				LEFT JOIN (SELECT
					cdoc.id,
					xmlelement(name document,
						xmlattributes(cdoc.sous_type AS "type", base.url || doc.code AS url)
					) AS xml_doc
					FROM
						remocra.document doc
						JOIN remocra.crise_document cdoc ON(cdoc.document = doc.id),
						(SELECT replace(valeur || '/telechargement/document/','//','/') AS url FROM remocra.param_conf WHERE cle = 'PDI_URL_SITE') AS base
					) AS d ON (d.id = cs.crise)
			WHERE
				creation BETWEEN to_timestamp('${DATE_HEURE_DEBUT}', 'YYYY-MM-DD HH24:MI:SS') AND coalesce(to_timestamp('${DATE_HEURE_FIN}', 'YYYY-MM-DD HH24:MI:SS'),now())
			ORDER BY
				creation
			) AS message
			GROUP BY
				crise,date_message, jour_message
			ORDER BY
				crise, date_message
		) AS messages_du_jour
		GROUP BY
			crise
	) AS cs  ON(cs.crise = c.id)
</sql>
