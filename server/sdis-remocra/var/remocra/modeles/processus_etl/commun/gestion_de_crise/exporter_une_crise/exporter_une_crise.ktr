    <connection>Remocra</connection>
    <sql>SELECT&#xa;	xmlelement&#x28;name &#x22;historiqueCrise&#x22;,&#xa;		xmlattributes&#x28;&#xa;				to_char&#x28;to_timestamp&#x28;&#x27;&#x24;&#x7b;DATE_HEURE_DEBUT&#x7d;&#x27;, &#x27;YYYY-MM-DD HH24&#x3a;MI&#x3a;SS&#x27;&#x29; ,&#x27;DD&#x2f;MM&#x2f;YYYY&#x27;&#x29; AS &#x22;dateDebutExport&#x22;,&#xa;				to_char&#x28;to_timestamp&#x28;&#x27;&#x24;&#x7b;DATE_HEURE_DEBUT&#x7d;&#x27;, &#x27;YYYY-MM-DD HH24&#x3a;MI&#x3a;SS&#x27;&#x29; ,&#x27;HH24&#x3a;MI&#x27;&#x29; AS &#x22;heureDebutExport&#x22;,&#xa;				to_char&#x28;coalesce&#x28;to_timestamp&#x28;&#x27;&#x24;&#x7b;DATE_HEURE_FIN&#x7d;&#x27;, &#x27;YYYY-MM-DD HH24&#x3a;MI&#x3a;SS&#x27;&#x29;,now&#x28;&#x29;&#x29;,&#x27;DD&#x2f;MM&#x2f;YYYY&#x27;&#x29; AS &#x22;dateFinExport&#x22;,&#xa;				to_char&#x28;coalesce&#x28;to_timestamp&#x28;&#x27;&#x24;&#x7b;DATE_HEURE_FIN&#x7d;&#x27;, &#x27;YYYY-MM-DD HH24&#x3a;MI&#x3a;SS&#x27;&#x29;,now&#x28;&#x29;&#x29;,&#x27;HH24&#x3a;MI&#x27;&#x29; AS &#x22;heureFinExport&#x22;&#xa;		&#x29;,&#xa;		xmlelement&#x28;&#xa;			name crise,&#xa;			xmlattributes&#x28;&#xa;				to_char&#x28;activation,&#x27;DD&#x2f;MM&#x2f;YYYY&#x27;&#x29; AS &#x22;dateActivation&#x22;,&#xa;				to_char&#x28;activation,&#x27;HH24&#x3a;MI&#x27;&#x29; AS &#x22;heureActivation&#x22;,&#xa;				to_char&#x28;coalesce&#x28;cloture,now&#x28;&#x29;&#x29;,&#x27;DD&#x2f;MM&#x2f;YYYY&#x27;&#x29; AS &#x22;dateCloture&#x22;,&#xa;				to_char&#x28;coalesce&#x28;cloture,now&#x28;&#x29;&#x29;,&#x27;HH24&#x3a;MI&#x27;&#x29; AS &#x22;heureCloture&#x22;&#xa;			&#x29;,&#xa;			xmlelement&#x28;name nom, c.nom&#x29;,&#xa;			xmlelement&#x28;name description, c.description&#x29;&#xa;			&#xa;		&#x29;,&#xa;		cs.messages&#xa;	&#x29; AS historique_crise_xml&#xa;FROM&#xa;	&#x28;SELECT &#x2a; FROM remocra.crise WHERE id &#x3d; &#x24;&#x7b;CRISE_ID&#x7d;&#x29; c&#xa;	JOIN&#x28;&#xa;		-- Messages par crise&#xa;		SELECT&#xa;			crise,&#xa;			xmlagg&#x28;messages_du_jour&#x29; AS messages&#xa;		FROM&#xa;		-- Messages par jour&#xa;		&#x28;SELECT&#xa;			crise,&#xa;			xmlelement&#x28;name messages, xmlattributes&#x28;jour_message AS &#x22;dateCreation&#x22;&#x29;,xmlagg&#x28;message&#x29;&#x29; AS messages_du_jour&#xa;		FROM&#xa;			&#x28;SELECT&#xa;				crise,&#xa;				cs.creation&#x3a;&#x3a;date AS date_message,&#xa;				to_char&#x28;cs.creation,&#x27;DD&#x2f;MM&#x2f;YYYY&#x27;&#x29; AS jour_message,&#xa;				xmlelement&#x28;name message,&#xa;					xmlattributes&#x28;to_char&#x28;cs.creation,&#x27;HH24&#x3a;MI&#x27;&#x29; AS &#x22;heureCreation&#x22;&#x29;,&#xa;					xmlelement&#x28;name objet,cs.objet&#x29;,&#xa;					xmlelement&#x28;name corps,&#x28;&#x27;&#x3c;ligne&#x3e;&#x27;&#x7c;&#x7c; regexp_replace&#x28;cs.message, E&#x27;&#x5b;&#x5c;&#x5c;n&#x5c;&#x5c;r&#x5d;&#x2b;&#x27;,&#x27;&#x3c;&#x2f;ligne&#x3e;&#x3c;ligne&#x3e;&#x27;, &#x27;g&#x27;&#x29; &#x7c;&#x7c; &#x27;&#x3c;&#x2f;ligne&#x3e;&#x27;&#x29;&#x3a;&#x3a;xml&#x29;,&#xa;					xml_doc&#xa;				&#x29; AS message&#xa;				&#xa;			FROM&#xa;				remocra.crise_suivi cs&#xa;				LEFT JOIN &#x28;SELECT&#xa;					doc.id,&#xa;					xmlelement&#x28;name document,&#xa;						xmlattributes&#x28;cdoc.sous_type AS &#x22;type&#x22;, base.url &#x7c;&#x7c; doc.code AS url&#x29;&#xa;					&#x29; AS xml_doc&#xa;					FROM&#xa;						remocra.document doc&#xa;						JOIN remocra.crise_document cdoc ON&#x28;cdoc.document &#x3d; doc.id&#x29;,&#xa;						&#x28;SELECT replace&#x28;valeur &#x7c;&#x7c; &#x27;&#x2f;telechargement&#x2f;document&#x2f;&#x27;,&#x27;&#x2f;&#x2f;&#x27;,&#x27;&#x2f;&#x27;&#x29; AS url FROM remocra.param_conf WHERE cle &#x3d; &#x27;PDI_URL_SITE&#x27;&#x29; AS base&#xa;					&#x29; AS d ON &#x28;d.id &#x3d; cs.document&#x29;&#xa;			WHERE&#xa;				creation BETWEEN to_timestamp&#x28;&#x27;&#x24;&#x7b;DATE_HEURE_DEBUT&#x7d;&#x27;, &#x27;YYYY-MM-DD HH24&#x3a;MI&#x3a;SS&#x27;&#x29; AND coalesce&#x28;to_timestamp&#x28;&#x27;&#x24;&#x7b;DATE_HEURE_FIN&#x7d;&#x27;, &#x27;YYYY-MM-DD HH24&#x3a;MI&#x3a;SS&#x27;&#x29;,now&#x28;&#x29;&#x29;&#xa;			ORDER BY&#xa;				creation&#xa;			&#x29; AS message&#xa;			GROUP BY&#xa;				crise,date_message, jour_message&#xa;			ORDER BY&#xa;				crise, date_message&#xa;		&#x29; AS messages_du_jour&#xa;		GROUP BY&#xa;			crise&#xa;	&#x29; AS cs  ON&#x28;cs.crise &#x3d; c.id&#x29;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
